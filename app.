
 theurl =() => {

    let pronoun = ['the', 'El'];
    let adj = ['super', 'maravilloso', 'gran', 'nefasto']
    let sujet = ['pez', 'micrero', 'computador', 'internet']
    let extension = ['.com', '.cl', '.net']

    let pronounA = Math.floor(Math.random() * pronoun.length);
    let adjA = Math.floor(Math.random() * adj.length);
    let sujetA = Math.floor(Math.random() * sujet.length);
    let extensionA = Math.floor(Math.random() * extension.length);


    return pronoun[pronounA] + '' + adj[adjA] + '' + sujet[sujetA] + '' + extension[extensionA];
}

console.log(theurl());




